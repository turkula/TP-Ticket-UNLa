<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="datos.Sector" table="sector">
		<id column="idSector" name="idSector">
			<generator class="identity" />
		</id>
		<property column = "descripcion" name = "descripcion" type = "string" />
		<many-to-one name="auditorio" class="datos.Auditorio"
					 column="idAuditorio" not-null="true" />
		<!--<joined-subclass name="datos.Numerada" table="numerada">-->
			<!--<key column="idNumerada" />-->
			<!--<property column="idButaca" name="idButaca" type="int"/>-->
		<!--</joined-subclass>-->
		<joined-subclass name="datos.Popular" table="popular">
			<key column="idPopular" />
			<property column="cantidadMaxima" name="cantidadMaxima" type="int"/>
		</joined-subclass>
	</class>
</hibernate-mapping>